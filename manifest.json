{
    "name": "Video Duration Playback Time",
    "short_name": "PlaybackTime",
    "description": "Shows the actual playback duration of sped up videos on YouTube",
    "version": "2.0.0",
    "manifest_version": 3,
     "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "js": ["scripts/youtube.js"],
            "matches": [
                "*://www.youtube.com/*"
            ]
        }
    ]
}